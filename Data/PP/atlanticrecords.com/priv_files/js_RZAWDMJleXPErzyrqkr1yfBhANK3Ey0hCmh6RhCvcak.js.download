WMG.namespace('userAgent');
WMG.userAgent = (function(){
    var os = 'other';
    var isMobile = false;
    if (/iPhone|iPod|iPad/i.test(navigator.userAgent)) {
        os = 'iOS';
        isMobile = true;
    }
    if (/Android/i.test(navigator.userAgent)) {
        os = 'Android';
        isMobile = true;
    }
    return {
        os: os,
        isMobile: isMobile
    }
})();
