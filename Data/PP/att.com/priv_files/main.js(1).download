        if (windowsize < 768) {
            $('.tab-titles').css('display', 'none');
            $('.tab-nav .menuButton').parents('.component').removeClass('csstransforms3d csstransitions js-tab-nav');
        } else {
            $('.tab-titles').css('display', 'block');
