	var _getLocaleCountryCode = function() {
        var url = window.location.pathname;
        var loc, m = url.match(/\/content\/atvi\/activision\/web\/(\w+)\/.*/);
        if(m) {
			loc = m[1].replace(/_/, "").toUpperCase();
            switch(loc) {
                case "EN":
                    loc = "US"; break;
                case "ENGB":
                    loc = "UK"; break;
                case "ENAU":
                    loc = "AU"; break;
            }
        } else {
