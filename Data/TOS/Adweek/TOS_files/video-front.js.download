/* Article template */
#single-post .container-title h2 {
	font-size: 24px;
	line-height:29px;
}

.single-post #sidebar .widget .container-title {
    padding: 0;
}

.single-post #sidebar .widget .container-title h2 { width: 100%;
}

.interstitial-container .aw-container:after { height: 0; }

/* Index pages */
.container-title .subtitle {
	display: block;
	margin-bottom: 10px!important;
}


.container-meno #section-3 .aw-element:after { background: none; }

.post-well .social span, .entry-header .social, .social-article-footer div, .addthis_toolbox, #sticky-nav .social p {display:none !important;}

#floating-300 {
	width: 100%;
        max-width:1020px;
        margin: 0 auto;
	margin-bottom: 20px;
}
#floating-300 .vc_col-sm-9 {
	float: left;
	width: calc(100% - 340px);
	min-width: 720px;
}
#floating-300 .vc_col-sm-9 .vcfw-link h2 { 
        font-size:20px;
        line-height:28px;
}
#floating-300 .vc_col-sm-3 {
	float: left;
	width: 300px;
}
@media screen and (max-width: 1040px) {
#floating-300 .vc_col-sm-9 {
	float: none;
        margin:0 auto;
        margin-bottom:20px;
}
#floating-300 .vc_col-sm-3 {
	float: none;
        margin: 0 auto;
        width: 300px;
        margin-bottom: 20px;
}
}
@media screen and (max-width: 680px) {
#floating-300 .vc_col-sm-9 {
	min-width:300px;
}
}

/* 21:9 images going to 16:9 */
@media screen and (max-width: 86.1875em){
.container-meno .ar-21-9 img,
.aw-element.full-width .vcfw-image img, #single-post .aw-element.full-width .vcfw-image img{    
        height: 100%;
	    width: auto;
    }
}

/* magazine width */
.hero-magazine-width {
    max-width: 640px;
    float: right;
}
/* Quick fix on blockquote */
@media screen and (min-width: 1055px){
	#single-post .main-content blockquote {	
    margin-left: calc( 100% - 640px);
	}
}
#single-post .main-content blockquote {
	padding-left: 24px !important;
	padding-top: 0px;
}

.kickout .ko-embed .instagram-media {
	position: relative;

}

/* Polar Fix  */
.kickout .plr-byline, .sticky .plr-byline {
    display:none;
}

.footer-campaign .campaign-logo img {
	max-height:100px;
}

/* Temp Lytics Slideout fix */
@media screen and (max-width: 53.0625em) {

	body .1adweek.pf-widget.webinar.pf-widget-slideout {
	    width: 340px;
	    height: 270px;
	    top: calc(100% - 339px);
	    bottom: auto;
	}
	
	
}

/* TEMP Event Fix */
.single-event .wpcf7-form select {
    width: 100%;
}

.single-event .aw-container h3 {
	margin-bottom: 0;
	margin-top: 25px;
}

