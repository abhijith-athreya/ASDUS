define('common/video/videoplaylist', ['jquery', 'common/carousel/touchcarousel'], function($, touchcarousel) {

  var videoplaylist = function() {

    this.createVideoPlaylist = function (playlistId) {
      if (!aetn.video.videoplaylist) {
        aetn.video.videoplaylist = [];
      }
      
      var videoList = $('.videoThumbNail[data-module-id=' + playlistId + ']');
      if (videoList.length > 0) {
        var playlist = {
            'playlistId' : playlistId
          , 'videoArray' : {}
          , 'length' : videoList.length
        }
        var video = {}
        , previousVideo = ''
        , freeVideoArray = [];
        videoList.each( function (eachItem) {
          video = {};
          var videoItem = $(videoList[eachItem]);
          video.id = videoItem.data().id;
          video.href = videoItem.data().href;
          video.releaseUrl = videoItem.data().releaseUrl;
          video.status = videoItem.data().status;
          video.title = videoItem.data().originalTitle;
          video.description = videoItem.data().content;
          video.duration = videoItem.data().duration;
          video.rating = videoItem.data().rating;
          video.airDate = videoItem.data().airDate;
          if (previousVideo.toString().length > 0) {
            playlist.videoArray[previousVideo].nextVideo = video.id;
          }
          if (video.status != 'locked') {
            freeVideoArray.push(video);
          }
          playlist.videoArray[video.id] = video;
          previousVideo = video.id;
        });
        // console.log(freeVideoArray);
        for (var i=0; i < freeVideoArray.length; i++) {
          if (i+1 < freeVideoArray.length) {
            playlist.videoArray[freeVideoArray[i].id].nextFreeVideo = freeVideoArray[i+1].id;
          }
        }
        for (var y=0; y < aetn.video.videoplaylist.length;y++) {
          if (aetn.video.videoplaylist[y].playlistId == playlistId) {
            aetn.video.videoplaylist.splice(y, 1);
          }
        }

        aetn.video.videoplaylist.push(playlist);
      }

    };

  };

  return videoplaylist;
});

define('common/ui/slideupeffect', ['jquery'], function($) {

  function slideupeffect() {
    /* SLIDE IN EFFECT FOR RIGHT RAIL */
    /* visible plugin */
    (function($) {

      /**
       * Copyright 2012, Digital Fusion
       * Licensed under the MIT license.
       * http://teamdf.com/jquery-plugins/license/
       *
       * @author Sam Sehnert
       * @desc A small plugin that checks whether elements are within
       *     the user visible viewport of a web browser.
       *     only accounts for vertical position, not horizontal.
       */

      $.fn.visible = function(partial) {
        
          var $t            = $(this),
              $w            = $(window),
              viewTop       = $w.scrollTop(),
              viewBottom    = viewTop + $w.height(),
              _top          = $t.offset().top,
              _bottom       = _top + $t.height(),
              compareTop    = partial === true ? _bottom : _top,
              compareBottom = partial === true ? _top : _bottom;
        
        return ((compareBottom <= viewBottom) && (compareTop >= viewTop));

      };
        
    })(jQuery);
    /* visible plugin */

    var slideInEls = $(".slide-in");

    slideInEls.each(function(i, el) {
      var el = $(el);
      if (el.visible()) {
        el.addClass("already-visible"); 
      } 
    });

    $(window).scroll(function(event) {
      
      slideInEls.each(function(i, el) {
        var el = $(el);
        if (el.visible()) {
          el.addClass("come-in"); 
        } 
      });
      
    });
    /* END SLIDE IN EFFECT FOR RIGHT RAIL */
  }
  
  return slideupeffect;
});
