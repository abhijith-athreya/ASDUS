define('common/international/geopopup', ['jquery', 'jquery.cookie', 'jquery.colorbox'], function($) {
  function geoRedirect() {

    this.init = function(overlayHtml, affiliateCountries, overlayDiv) {
      var countryCode = getCountryCode();
      var countryInfo = getAffiliateCountryInfo(countryCode, affiliateCountries);
   
      if (countryInfo) {
          if ( $.cookie('country_code') == null) {
                initOverlay(countryInfo, overlayHtml);
                $.colorbox({inline :true, href : overlayDiv, fixed: true});
          }            
      }

    }

    function getCountryCode() {
          var url = window.location.href;
          if(url.indexOf("?QA-Country-Code=") != -1) {
            return url.substring(url.indexOf("?QA-Country-Code=") + 17);
          } else {
            var httpCountryCode = $.cookie('Country-Code');
            return httpCountryCode;
          }
    }

    function getAffiliateCountryInfo(countryCode, countries) {
      var countryInfo;
      for (var i = 0; i < countries.length; i++) {
            if (countries[i].countryCode == countryCode) {
                countryInfo = countries[i];
                break;
            }
          }
          return countryInfo;
    }

    function initOverlay(countryInfo, overlayHtml) {
    
          $('body').append(overlayHtml);

          $('#welcomeMsg').html(countryInfo.welcomeMsg);
          $('#usaMsg').html(countryInfo.usaMsg);
          $('#countryMsg').attr('href', countryInfo.countryUrl).html(countryInfo.countryMsg);
        
          $('#countryMsg').bind('click', function(e) {
            $.cookie('country_code', countryInfo.countryCode); //set cookie to indicated user pick other country
          });

          $('#usaMsg').bind('click', function(e) {
            e.preventDefault();
            $.fn.colorbox.close();
          });
    }

  }

  return geoRedirect;

});
