define('common/ui/privacy', ['jquery', 'jquery.cookie'], function($) {

  var privacyNotice = {
    storageName : 'ae_policy_notice',

    hasStorage : false,

    init: function() {
      this.checkStorage();
      this.checkStatus();
    },

    checkStorage: function() {
      if(typeof Storage !== 'undefined') {
        this.hasStorage = true;
      }
      else {
        this.hasStorage = false;
      }
    },

    checkStatus: function() {
      if ( !this.getStatus() ) {
        this.displayNotice();
        this.setListener();
      }
    },

    getStatus: function() {
      if( this.hasStorage ) {
        return localStorage.getItem( this.storageName );
      }
      else {
        return $.cookie( this.storageName );
      }
    },

    setStatus: function() {
      if( this.hasStorage ) {
        localStorage.setItem( this.storageName, true );
      }
      else {
