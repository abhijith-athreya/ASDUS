define('common/ui/lazyloadContent', ['jquery'], function($) {

  var lazyload = {
    options : {
      'ajaxUrl' : '',
      'pageParam' : 'page',
      'markerId' : '#preloader',
      'appendContainer' : '#articleInfinite'
    },

    isLoading : false,
    scrollTimeout : null,
    pageNo : 1,

    init: function( _options ) {
      $.extend( this.options, _options );
      this.setListeners();
      this.checkInView();
    },

    setListeners: function() {
      var self = this;

      $(document).on( 'scroll', function () {
        if ( self.scrollTimeout ) {
          clearTimeout( self.scrollTimeout );
        }
        self.scrollTimeout = setTimeout(function() {
          self.checkInView();
        }, 250);

      });

      $(document).on( 'lazyloadContent', function() {
        if ( self.isLoading ) return false;
        self.isLoading = true;
        $.ajax({
            url: self.options.ajaxUrl + self.options.pageParam + '=' + (++self.pageNo),
            dataType: "json"
        })
        .done(function ( response ) {
          self.isLoading = false;
          if ( $.isEmptyObject( response ) || !response.aetn_content ) {
            $(self.options.markerId).hide();
          }
          else {
            $(self.options.appendContainer).append( response.aetn_content.html ? response.aetn_content.html : response.aetn_content );
  
            if ( response.jsExec ) {
              eval( response.jsExec );
            }
          }
        });
      });
      
    },

    checkInView: function() {
      if ( $(this.options.markerId + ':visible' ).length < 1 ) return;
      var markerTop = $(this.options.markerId + ':visible' ).offset().top;

      if ( markerTop >= $(window).scrollTop() && markerTop < $(window).scrollTop() + $(window).height() ) {
        $(document).trigger( 'lazyloadContent' );
      }
    }
  };

  return lazyload;

});

define('common/carousel/hero', ['jquery'], function($) {

  function hero(id) {
    var heroCarousel = $('#' + id).carousel();

    //Set the first dot as active
    $('#' + id + ' a.carousel-page').removeClass('active').eq(0).addClass('active');

    heroCarousel.bind('slid', function() {
      var index = $('.carousel-inner').find('.active').index();
      $(this).find("a.carousel-page").removeClass('active').eq(index).addClass('active');
    });

    $('.carousel-pager a').click(function(e){
      e.preventDefault();
      var index = $(this).index();
      heroCarousel.carousel(index);
    });
  }

  return hero;
});
define('common/carousel/slider', ['jquery'], function($) {

  function slider(id) {
    $('#' + id).carousel({
      interval: false
    });

    $('#' + id).bind('slide', function() {
      $(this).removeClass('static');
      populateImages();
    });

    $('#' + id).bind('slid', function() {
      $(this).addClass('static');
      //populateImages(); 

    });

    function populateImages() {

      var next = $('#' + id + ' ul.active').next().not($('.alreadyset'));
      var prev = $('#' + id + ' ul.active').prev().not($('.alreadyset'));

      if (next) {
        $('img', next).map(function() {
          if(!this.src) {
            this.src = $(this).attr("data-src");
          }
        });

        $(next).addClass("alreadyset");
      }

      if (prev) {
        $('img', prev).map(function() {
          if(!this.src) {
            this.src = $(this).attr("data-src");
          }
        });
        $(prev).addClass("alreadyset");
      }
    }

  }

