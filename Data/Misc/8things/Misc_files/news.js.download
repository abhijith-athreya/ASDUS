 *   leftControlSelector: CSS3 selector for left control.(Default: '.slider-control.left')
 *   rightControlSelector: CSS3 selector for right control.(Default: '.slider-control.right')
 *   }
 */

/*
 * TODO: Functionality to initialize carousel only when have more then one pane.
 */

function touchcarousel(element, options) {

  var self = this;
  
  this.elementId = element;
  this.element = $('#' + element);
  this.debugEl = $('#debug');

  var _defaultOptions = {
      listsSelector: 'ul'
    , listsItemSelector: 'li'
    , implementTouch: true
    , implementCarousel: true
    , nextCallback: null
    , prevCallback: null
    , leftControlSelector: '.slider-control.left'
    , rightControlSelector: '.slider-control.right'
    , carouselPagerSelector: 'div.carousel-pager'
    , carouselPagerItemSelector: 'a.carousel-page'    
  };
  var options = options? options: {};
  options = $.extend(_defaultOptions, options);

  this.container = $(options.listsSelector, this.element);
  if (this.container.length > 1) {
    this.container = $(this.container[0]);
  }
  var panes = $(options.listsItemSelector, this.container);

  var pane_width = 0;
  var pane_count = 0;
  var each_pane_item_count = 0;

  var current_pane = 0;

  /**
   * initialize
   */
  this.init = function() {
    if (options.implementCarousel) {
      _setPaneDimensions();

      $('#' + this.elementId + ' ' + options.leftControlSelector).hide();

      if (pane_count == 1) {
        $('#' + this.elementId + ' ' + options.rightControlSelector).hide();
      }

      $(document).on("click", '#' + this.elementId + ' ' + options.leftControlSelector, function(ev) {
        ev.preventDefault();
        self.prev();
        if (current_pane == 0) {
          $('#' + self.elementId + ' ' + options.leftControlSelector).hide();
        }
        $('#' + self.elementId + ' ' + options.rightControlSelector).show();

      });

      $(document).on("click", '#' + this.elementId + ' ' + options.rightControlSelector, function(ev) {
        ev.preventDefault();
        self.next();
        if (current_pane == pane_count -1) {
          $('#' + self.elementId + ' ' + options.rightControlSelector).hide();
        }
        $('#' + self.elementId + ' ' + options.leftControlSelector).show();
      });

      $(window).on("load resize orientationchange", function() {
        _setPaneDimensions();
      });
    }
  };

  /**
   * set the pane dimensions and scale the container
   */
  function _setPaneDimensions() {
    pane_width = self.element.width();

    var _paneCount = panes.length/(parseInt(pane_width/$(panes[0]).width()));

    if (_paneCount > parseInt(_paneCount)) {
      pane_count = parseInt(_paneCount)+1;
    } else {
      pane_count = _paneCount;
    }
    //hide carousel pager if only one pane.
    if ((pane_count-1 == 0) || (pane_count-1 < 0)) {
      _setCarouselPager(null);
    }
    each_pane_item_count = parseInt(pane_width/$(panes[0]).width());

    if (self.debugEl) {
      self.debugEl.html('pane_width, pane_count:' + pane_width + ',' + pane_count);
    }
    
  };

  /*
   * set offset for container after swipe/drag event has occured. 
   */
  function _setContainerOffset(percent, container, animate) {
    container.removeClass("animate");

    if(animate) {
      container.addClass("animate");
    }

    var px = (pane_width*(percent/100));
    
    // Disbaling csstransforms for android devices. 
    // As for native android browser transformations doesn't work as expected.
    
    if (navigator.userAgent.match(/android/i)) {
      container.css("transform-origin", "0,0,0");
      container.css("left", px+"px"); 
    
    } else {
      if(Modernizr.csstransforms3d) {
        container.css("transform", "translate3d("+ percent +"%,0,0) scale3d(1,1,1)");
      }
      else 
      if(Modernizr.csstransforms) {
        container.css("transform", "translate("+ percent +"%,0)");
      }
      else {
        container.css("transform-origin", "0,0,0");
        container.css("left", px+"px");
      }  
    }

    
  }


  function _handleHammer(ev) {
   // console.log(ev);
    // disable browser scrolling
    ev.gesture.preventDefault();

    switch(ev.type) {
      case 'swipeleft':
        self.next();
        ev.gesture.stopDetect();
        break;

      case 'swiperight':
        self.prev();
        ev.gesture.stopDetect();
        break;
      case 'dragleft':
        self.next();
        ev.gesture.stopDetect();
        break;

      case 'dragright':
        self.prev();
        ev.gesture.stopDetect();
        break;

      case 'release':
        // more then 50% moved, navigate
        if(Math.abs(ev.gesture.deltaX) > pane_width/2) {
            if(ev.gesture.direction == 'right') {
                self.prev();
            } else {
                self.next();
            }
        }
        else {
            self.showPane(current_pane, true);
        }
        break;
    }
  }

  function _setCarouselPager(selectIndex) {

    switch(selectIndex) {
      case 'first':
        $('#' + self.elementId + ' ' + options.leftControlSelector).hide();
        if (pane_count > 1) {
          $('#' + self.elementId + ' ' + options.rightControlSelector).show();
        }
          break;
      case 'middle':
          if (pane_count > 1) {
            $('#' + self.elementId + ' ' + options.leftControlSelector).show();
            $('#' + self.elementId + ' ' + options.rightControlSelector).show();
          }
        break;
      case 'last':
        $('#' + self.elementId + ' ' + options.rightControlSelector).hide();
        if (pane_count > 1) {
          $('#' + self.elementId + ' ' + options.leftControlSelector).show();
        }
        break;
    }

    var _carouselPagerEl = $(options.carouselPagerSelector, self.element);
    var _carouselPageItems = $(options.carouselPagerItemSelector, self.element);
    //hide pager 
    if (_carouselPagerEl) {
      if (!selectIndex) {
        _carouselPagerEl.addClass('hidden');
      } else {
        if (_carouselPageItems) {
          //set first carousel page
          _carouselPageItems.removeClass('active');
          if (selectIndex === 'first' && _carouselPageItems[0]) {
            $(_carouselPageItems[0]).addClass('active');
          } else if (selectIndex === 'last' && _carouselPageItems[2]) {
            $(_carouselPageItems[2]).addClass('active');
          } else if (_carouselPageItems[1]) {
            $(_carouselPageItems[1]).addClass('active');
          }
        }
      }
    }
  }

  /**
   * show pane by index
   * @param   {Number}    index
   */
  this.showPane = function( index ) {
    // between the bounds
    index = Math.max(0, Math.min(index, pane_count-1));

    if (index == 0) {
      _setCarouselPager('first');
    } else if (index == pane_count-1) {
      _setCarouselPager('last');
    } else {
      _setCarouselPager('middle');
    }

    current_pane = index;

    var offset = -(100*current_pane);

    this.container = $(options.listsSelector, this.element);
    if (this.container.length > 1) {
      this.container = $(this.container[0]);
    }
    _setContainerOffset(offset, this.container, true);
  };

  this.populateImages = function(current_pane, end_pane) {
    var _unloadedPane = (current_pane+2);
    var _unloadedPaneStart = _unloadedPane*each_pane_item_count;
    var _unloadedPaneEnd = end_pane? end_pane*each_pane_item_count: _unloadedPaneStart+each_pane_item_count;
    
    for (var i = _unloadedPaneStart; i < _unloadedPaneEnd; i++) {
      if (panes.length > i) {
        var _imgTag = panes[i].getElementsByTagName('img');
        var _imgTagEl = $(_imgTag);
        if (!_imgTagEl.attr('src')) {
          if (_imgTagEl.data('src')) {
            //data-src tag found
            var _src = _imgTagEl.data('src');
            _imgTagEl.attr('src', _src);
          }
        }
      }
    };
  };

  this.scrollToIndex = function(index) {
    var _swipeToPane = parseInt(index/each_pane_item_count);
    this.populateImages(current_pane, _swipeToPane+2);
    this.showPane(_swipeToPane);
  };

  if (options.nextCallback) {
    this.next = options.nextCallback;
  } else {
    this.next = function() { 
      this.populateImages(current_pane);
      return this.showPane(current_pane+1, true); 
    };

  }

  if (options.prevCallback) {
    this.prev = options.prevCallback;
  } else {
    this.prev = function() { return this.showPane(current_pane-1, true); };

  }

  if (options.implementTouch) {
    this.element.hammer().on("release dragleft dragright swipeleft swiperight", _handleHammer);    
  }


}



$.fn.touchcarousel = function(options) {

  return this.each(function() {
      var el = $(this);
      var inst = el.data('touchcarousel');
      // start new touchcarousel instance
      if(!inst) {
          var carousel = new touchcarousel(this.id, options || {});
          carousel.init();
          el.data('touchcarousel', carousel);
      }
      // change the options
      else if(inst && options) {
          touchcarousel.utils.extend(inst.options, options);
      }
  });
};
return touchcarousel;

});


