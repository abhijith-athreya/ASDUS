            this.grid.height += 125;
        }

        console.log( this.grid );
        return true;
    };


    var image = new Image();
    image.onload = function() {

      $('#container').isotope({

          itemSelector : '.item',
          layoutMode : 'grid',
          grid : {
            min_height : 136,
            min_width : 238
          },
          getSortData : {
            title : function ( elem ) {
            return elem.data('title');
            },
            newest : function ( elem ) {
            return parseInt (elem.data('lastvideoaddedat'), 10);
            },
            popular : function ( elem ) {
            return parseInt (elem.data('videovisits'), 10);
            }
          }
      });
    };

    image.src = $('#container li:not(.ad) img').first().attr( 'src' );


    $("#filterDropDown li").click(function(e) {

      e.preventDefault();

      var filterText =  $(this).find('a').text();

      $("#filter .selection").text(filterText);

      $('#container').isotope({ filter: $(this).find('a').data('filter') });

    });

    $("#sortDropDown li").click(function(e) {

      e.preventDefault();

      var sortID = $(this).find('a').attr('id');
      var sortText =  $(this).find('a').text();

      $("#sort .selection").text(sortText);

      switch(sortID){

        case 'sortAsc' :
          $('#container').isotope({ sortBy : 'title' , sortAscending : true });
          break;

        case 'sortDesc' :
          $('#container').isotope({ sortBy : 'title' , sortAscending : false });
          break;

        case 'sortNew' :
          $('#container').isotope({ sortBy : 'newest' , sortAscending : false });
          break;

        case 'sortPopular' :
          $('#container').isotope({ sortBy : 'popular' , sortAscending : false });
          break;

        default:
          break;
