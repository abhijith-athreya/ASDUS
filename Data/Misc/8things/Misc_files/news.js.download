define('common/video/watchvideoplayer', ['jquery'], function($) {

  function watchvideoplayer() {

    var pauseState = false;
    var media_id = $('.aetn-player').data('videoId');

    if (window.$pdk && window.$pdk.controller) {
      window.$pdk.controller.addEventListener('OnMediaStart', function() {
        $('.aetn-player-wrapper').removeClass('end-player');
      });
      window.$pdk.controller.addEventListener('OnMediaEnd', function() {
        $('.aetn-player-wrapper').addClass('end-player');
      });
      window.$pdk.controller.addEventListener('OnMediaUnpause', function() {
        $('.aetn-player-wrapper').removeClass('non-sticky');
        pauseState = false;
      });
      window.$pdk.controller.addEventListener('OnMediaPause', function() {
        pauseState = true;
      });

      window.$pdk.controller.addEventListener('OnReleaseStart', onReleaseStart);
      window.$pdk.controller.addEventListener('OnReleaseEnd', onReleaseEnd);


      $(window).scroll(function() {
        var playerOffset = $('.aetn-player-wrapper').offset().top + ($('.aetn-player-wrapper').outerHeight() / 2);
        if ($(window).scrollTop() > playerOffset) {
          if (pauseState !== true) {
            $('.aetn-player-wrapper:not(.end-player)').addClass('sticky-player');
            window.dispatchEvent(new Event('resize'));
          }
        } else {
          $('.aetn-player-wrapper').removeClass('sticky-player');
          window.dispatchEvent(new Event('resize'));
        }
      });
      $('.aetn-player-wrapper .sticky-player-close').on('click', function() {
        $('.aetn-player-wrapper').addClass('non-sticky');
          window.$pdk.controller.pause(true);
        if (window.utag) {
          window.utag.link({event_name: 'video_player_close'});
        }
        return false;
      });
    }

    function onReleaseStart() {

      if (aetn.video.videoplaylist && aetn.video.videoplaylist.length > 0) {

        var currentItem = $('.videoThumbNail[data-id=' + media_id + ']');
        var carousel = currentItem.parents('.slider');

        carousel.data('touchcarousel').scrollToIndex(currentItem.parents('li').index());

        var playlist = aetn.video.videoplaylist[0].videoArray;
        var currentVideo = playlist[media_id];

        if (currentVideo) {
          setVideoTitle(currentVideo);
        }

        $('.videoThumbNail').removeClass('active');
        $('.videoThumbNail[data-id=' + currentVideo.id + ']').addClass('active');
      }
    }

    function onReleaseEnd() {

      if (aetn.video.videoplaylist && aetn.video.videoplaylist.length > 0) {

        var playlist = aetn.video.videoplaylist[0].videoArray;
        var currentVideo = playlist[media_id];
        var nextVideo = currentVideo ? playlist[currentVideo.nextVideo] : null;

        nextVideo.releaseUrl = nextVideo.releaseUrl.replace(/^http:\/\//i, 'https://');

        if (nextVideo) {
          window.location.href = nextVideo.href;
        }
      }

    }

    function setVideoTitle(video) {

      $('.currentVideo-title').html('<h1>' + video.title + '</h1> <small>(' + video.duration + ')</small> <small class="uppercase">' + video.rating + '</small>');
      $('#currentVideo-description').html(video.description.replace('hideVideoPlayer', 'hidden'));

      $('.videoThumbNail').removeClass('active');
      $('.videoThumbNail[data-id=' + video.id + ']').addClass('active');
    }


  }

  return watchvideoplayer;
});

