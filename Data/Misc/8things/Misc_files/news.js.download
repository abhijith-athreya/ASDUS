        this.options.grid.columnWidth = $( element ).outerWidth(true);
        this.options.grid.rowHeight   = $( element ).outerHeight(true);

        this._getSegments();
        this._getSegments(true);

        this.grid.width  = this.element.width();

        if( this.grid.width >= 700 ) {
            this.grid.cols = 3;
        }
        else {
            this.grid.cols = 1;
        }

        this.grid.rows   = Math.ceil( this.$filteredAtoms.length / this.grid.cols );
