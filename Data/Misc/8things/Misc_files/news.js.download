            this.options.grid = {};
        }

        this._gridResizeChanged();
    };

    $.Isotope.prototype._gridLayout = function( $elems ) {

        var that  = this;
        var index = 0;

        $elems.each( function() {

            var $this = $(this);
            var x = 0;
            var y = 0;
            
            if( that.grid.width >= 1056 ) { // Desktop

                if( $this.hasClass( 'ad' ) ) { // Place ad in correct slot

                    x = 2 * that.grid.columnWidth;
                    y = 0;

                    that._pushPosition( $this, x, y );
                }
                else {

                    if( index === 2 || index === 5) { // Skip ad slot
                        index++;
                    }

                    x = (index % that.grid.cols) * that.grid.columnWidth;
                    y = Math.floor(index / that.grid.cols) * that.grid.rowHeight;

                    that._pushPosition( $this, x, y );
                    index++;
                }
            }
            else if( that.grid.width >= 700 && that.grid.width < 1056 ) { // Tablet

                if( $this.hasClass( 'ad' ) ) { // Place ad in correct slots

                    x = 0;
                    y = Math.floor(6 / that.grid.cols) * that.grid.rowHeight;

                    that._pushPosition( $this, x, y );
                }
                else {

                    if( index === 6 ) { // Skip ad slots
                        index = 9;
                    }

                    x = (index % that.grid.cols) * that.grid.columnWidth;

                    if( index > 8 ) {
                        y = ( ( ( Math.floor(index / that.grid.cols) ) - 1 ) * that.grid.rowHeight ) + 345;
                    } 
                    else {
                        y = Math.floor(index / that.grid.cols) * that.grid.rowHeight;
                    }

                    that._pushPosition( $this, x, y );
                    index++;
                }
            }
            else { // Mobile

                if( $this.hasClass( 'ad' ) ) { // Place ad in correct slots

                    x = 0;
                    y = 3 * that.grid.rowHeight;

                    that._pushPosition( $this, x, y );
                }
                else {

                    if( index === 3 ) {
                        index++;
                    }

                    x = 0;

                    if( index > 3 ) {
                        y = ( index * that.grid.rowHeight ) + 125;
                    }
                    else {
                        y = index * that.grid.rowHeight;
                    }

                    that._pushPosition( $this, x, y );
                    index++;
                }

            }
        });
