/* global Econ, jQuery, Drupal */

Econ.gpt = Econ.gpt || {};

(function(Econ, $, Drupal) {
  "use strict";
  //borrowed from ec_gpt_sticky.js
  Econ.gpt.stickyBillboard = function() {
    var leaderBoard = document.getElementById('leaderboard'),
        $leaderBoard = $(leaderBoard),
        $leaderBoardInner = $('#block-ec_ads-leaderboard_ad'),
        $leaderBoardClone = $('<div id="fixedAdBlockClone"></div>'),
        $cookieMessageContainer = $('#ec-cookie-messages-container'),
        leaderBoardHeight,
        leaderBoardAdvertHeight,
        leaderBoardInner,
        navigationBottom,
        fixStopPoint,
        cookieMessageHeight,
        pencilSlugHeight,
        $window = $(window);

    function loadDimensions() {
        cookieMessageHeight = parseInt($('#ec-cookie-messages-container').outerHeight(true), 10)  || 0;
        leaderBoardHeight = parseInt($('#leaderboard').outerHeight(true)) || 0;
        leaderBoardAdvertHeight = parseInt($('#block-ec_ads-leaderboard_ad').outerHeight(true)) || 0;
        navigationBottom = parseInt($('header.mh').outerHeight() + $('#leaderboard').outerHeight())  || 0;
        fixStopPoint = parseInt(navigationBottom - $('#block-ec_ads-leaderboard_ad').outerHeight())  || 0;
        pencilSlugHeight = parseInt($('#block-ec_ads-pencil_slug_ad').outerHeight(true))  || 0;
    }

    loadDimensions();

    $leaderBoard.addClass('stickyBillboard').css({'min-height' : (leaderBoardAdvertHeight + cookieMessageHeight)});

     //solving the case it is a first time site visit and we have cookie message on screen
    if (cookieMessageHeight > 0){
        $leaderBoard.addClass('cookieMessageBillboard');
        $cookieMessageContainer.addClass("sticked_message");
        $leaderBoardInner.css({'padding-top' : cookieMessageHeight});
        $('.ec-cookie-message a.dismiss-messages').bind('click', function(e) {
            e.preventDefault();
            $leaderBoardInner.animate({'padding-top' : 0}, 600);
            var newAdHeight = leaderBoardHeight - cookieMessageHeight + (parseInt($('#block-ec_ads-pencil_slug_ad').css("top")) || 0);
            //alert(parseInt($('#block-ec_ads-pencil_slug_ad').css("top")) || 0);
            $leaderBoard.animate({'height' : newAdHeight}, 600);
            $leaderBoardClone.animate({'height' : (leaderBoardAdvertHeight + pencilSlugHeight - cookieMessageHeight)}, 600, function() {
                loadDimensions();
                $leaderBoard.css({'min-height' : leaderBoardAdvertHeight});
            });
            $leaderBoard.removeClass('cookieMessageBillboard');
        });
    }

    loadDimensions();
    
     //borrowed from ec_gpt_sticky.js
    if (!$('#fixedAdBlockClone').length) {
      $leaderBoardClone.css({'width':'100%','height':leaderBoardHeight});
      $leaderBoard.after($leaderBoardClone);
      leaderBoard.className = leaderBoard.className + " adsFixed";
    }  

    $window.bind('scroll', function() {
      var scrollTop = $window.scrollTop();
      if (scrollTop === 0) {
        $leaderBoard.removeClass("adShadow");
      }
      else {
        $leaderBoard.addClass("adShadow");
      }

      if ((scrollTop + leaderBoardHeight) > navigationBottom) {
        $('#block-ec_ads-pencil_slug_ad').css({'top' : (parseInt(navigationBottom - (scrollTop + leaderBoardHeight)))});
        var actualLeaderboardHeight = parseInt(navigationBottom - scrollTop);
        $leaderBoard.css({'height' : actualLeaderboardHeight});
        if (scrollTop > fixStopPoint){
          $leaderBoard.css({'position' : 'fixed', 'top' : fixStopPoint - scrollTop}).animate({'padding-top' : 0}, 600);
          $cookieMessageContainer.css({'top' : fixStopPoint - scrollTop});
        }
      }
      else {
        $('#block-ec_ads-pencil_slug_ad').removeAttr('style');
        $leaderBoard.css({'position' : '', 'height' : '', 'top' : ''});
        $cookieMessageContainer.css({'top' : 0});
      }

    });
  };

})(Econ, jQuery, Drupal);
