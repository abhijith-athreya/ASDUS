if (document.querySelector('.cv-footer-popup') == null && document.cookie.indexOf('newsletter_subscribed') == -1) {

    //Click old input so elements exist
    utils.waitForElement('#dwfrm_profile_customer_footeremail').then(function () {
        var scrollLoc = scrollY;
        //$('#dwfrm_profile_customer_footeremail').focus();
      
        var inputField = document.getElementById('dwfrm_profile_customer_footeremail');
        var e = document.createEvent('Event');
        e.initEvent("focus", true, true);
        inputField.dispatchEvent(e);
      
      	window.scrollTo(0, scrollLoc);
    });


    //Create overlay
    var cvOverlay = document.createElement('div');
    cvOverlay.classList.add('cv-overlay');

    var cvFooterPopup = document.createElement('div');
    cvFooterPopup.classList.add('cv-footer-popup');

    var cvFooterPopupTitle = document.createElement('div');
    cvFooterPopupTitle.classList.add('cv-footer-popup-title');

    var cvFooterPopupClose = document.createElement('span');
    cvFooterPopupClose.classList.add('cv-popup-close');



    //console.log('ATP-824');
    //Wait for title as it loads in slowest
    utils.waitForElement('.subscribe_title, label[class*="footer_newsletter"] span span, .footer-newsletter-form-title').then(function () {
      


        //Copy title and create div
        cvFooterPopup.appendChild(cvFooterPopupTitle);
        var cvFooterPopupLabel = document.createElement('label');
        cvFooterPopupLabel.innerHTML = document.querySelector('.subscribe_title, label[class*="footer_newsletter"] span span, .footer-newsletter-form-title').innerHTML;
        cvFooterPopupTitle.appendChild(cvFooterPopupLabel);
        cvFooterPopupTitle.appendChild(cvFooterPopupClose);


        //Wait for input field and then clone it, remove the ID
        utils.waitForElement('#dwfrm_profile_customer_footeremail, input[class*="footer_newsletter_subscribe"]').then(function () {
          

            if (document.getElementById('dwfrm_profile_customer_footeremail')) {
                var cvFooterPopupEmail = document.getElementById('dwfrm_profile_customer_footeremail').cloneNode(true);
                var cvFooterPopupSubmit = document.querySelector('.formactions').cloneNode(true);
            } else {
                cvFooterPopupEmail = document.getElementById('input[class*="footer_newsletter_subscribe"]').cloneNode(true);
                cvFooterPopupSubmit = document.querySelector('button[class*="footer_newsletter"]').cloneNode(true);
            }
            cvFooterPopupSubmit.classList.add('cv-footer-popup-submit');
            cvFooterPopupEmail.id = '';
            cvFooterPopupSubmit.querySelector('a').id = '';
            cvFooterPopupEmail.classList.add('cv-footer-popup-email');
            cvFooterPopup.appendChild(cvFooterPopupSubmit);
            cvFooterPopup.appendChild(cvFooterPopupEmail);

        });


        //Wait for legal message then copy it
        utils.waitForElement('.age_confirmation_text').then(function () {
            var cvFooterPopupMessage = document.createElement('label');
            cvFooterPopupMessage.innerHTML = document.querySelector('.age_confirmation_text').innerHTML;
            cvFooterPopup.appendChild(cvFooterPopupMessage);

            utils.waitForElement('#legalcontentpopup').then(function () {
                var cvLegalCopy = document.querySelector('.legal-copy-content').innerHTML;
                var cvLegalBlock = document.createElement('p');
                cvLegalBlock.classList.add('legal-copy-content');
                cvLegalBlock.innerHTML = cvLegalCopy;
                cvFooterPopup.appendChild(cvLegalBlock);
              	cvFooterPopup.style = 'display:block';
            });
          

        });


        //Add overlay to body
        cvOverlay.appendChild(cvFooterPopup);
        document.querySelector('body').appendChild(cvOverlay);

        utils.waitUntil(function () {
            return document.querySelectorAll('.legal-copy-content a').length > 1;
        }).then(function () {
            document.querySelectorAll('.legal-copy-content a')[1].addEventListener('click', function () {
                document.querySelectorAll('.legal-copy-content a')[0].click();
            });
        });

        //Close button
        utils.waitForElement('.cv-popup-close').then(function () {
            document.querySelector('.cv-popup-close').addEventListener('click', function () {
                document.querySelector('.cv-overlay').parentNode.removeChild(document.querySelector('.cv-overlay'));
                document.cookie = 'newsletter_subscribed=1;path=/;expires=Wed, 19 Dec 2018 23:59:59 GMT; domain='+window.location.host.replace('m.', '.') + ';';
            });
            document.querySelector('.cv-footer-popup-submit').addEventListener('click', function () {
                document.getElementById("footernewslettersubmitbutton").click();
                //Add function to wait for error messages
                addErrors();
                window['optimizely'] = window['optimizely'] || [];
                window['optimizely'].push({
                    type: "event",
                    eventName: "atp-824-signup-complete"
                });

                document.cookie = 'newsletter_subscribed=1;path=/;expires=Wed, 19 Dec 2018 23:59:59 GMT; domain='+window.location.host.replace('m.', '.') + ';';
            });
        });
        //Input field
        utils.waitForElement('.cv-footer-popup-email').then(function () {
          	document.querySelector('.cv-footer-popup-email').addEventListener('mousedown', function(){
              document.querySelector('.cv-footer-popup > label').style = 'display:block';
              document.querySelector('.cv-footer-popup > .legal-copy-content').style = 'display:block';
              
              if(document.querySelector('.cv-footer-popup .cv-popup-error')){
              	document.querySelector('.cv-footer-popup .cv-popup-error').style = 'display:block';
              }
              
              document.querySelector('.cv-overlay').style = 'height:100%!important';
            });
          	
            document.querySelector('.cv-footer-popup-email').addEventListener('keyup', function (event) {
                if (document.querySelector('.cv-footer-popup-email').value.length > 3) {
                    if (document.getElementById('dwfrm_profile_customer_footeremail')) {
                        document.getElementById('dwfrm_profile_customer_footeremail').value = document.querySelector('.cv-footer-popup-email').value;
                    } else {
                        document.querySelector('input[class*="footer_newsletter_subscribe"]').value = document.querySelector('.cv-footer-popup-email').value;
                    }
                    if (event.keyCode == 13) {
                        //Add function to wait for error messages
                        addErrors();
                        if (document.getElementById('dwfrm_profile_customer_footeremail')) {
                            document.getElementById("footernewslettersubmitbutton").click();
                        } else {
                            document.querySelector('input[class*="footer_newsletter_subscribe"]').click();
                        }
                        window['optimizely'] = window['optimizely'] || [];
                        window['optimizely'].push({
                            type: "event",
                            eventName: "atp-824-signup-complete"
                        });
                    }
                }
            });
          

        });
        //Check for succesful complete
        utils.waitUntil(function () {
            return document.querySelector('.signupandsave_complete').style.display != 'none';
        }).then(function () {
            document.querySelector('.cv-footer-popup').innerHTML = '';
            var cvFooterPopupClose = document.createElement('span');
            cvFooterPopupClose.classList.add('cv-popup-close');
            document.querySelector('.cv-footer-popup').innerHTML = document.querySelector('.signupandsave_complete').innerHTML;
            document.querySelector('.cv-footer-popup').appendChild(cvFooterPopupClose);
            document.querySelector('.cv-footer-popup').classList.add('cv-signup-complete');
            document.querySelector('.cv-overlay').style = 'height:24%!important';

            document.cookie = 'newsletter_subscribed=1;path=/;expires=Wed, 19 Dec 2018 23:59:59 GMT; domain='+window.location.host.replace('m.', '.') + ';';
            
            window['optimizely'] = window['optimizely'] || [];
            window['optimizely'].push({
                type: "event",
                eventName: "atp-824-signup-succesful"
            });

            document.querySelector('.cv-popup-close').addEventListener('click', function () {
                document.querySelector('.cv-overlay').parentNode.removeChild(document.querySelector('.cv-overlay'));
                document.querySelector('body').classList.remove('visibleDialog');
            });
        });

        //USE THIS TO FOCUS INPUT FIELD SO FORM ACTUALLY WORKS, otherwise it won't submit without errors.
        utils.waitForElement('#dwfrm_profile_customer_footeremail').then(function () {
           // $('#dwfrm_profile_customer_footeremail').focus();
          
            var inputField = document.getElementById('dwfrm_profile_customer_footeremail');
            var e = document.createEvent('Event');
            e.initEvent("focus", true, true);
            inputField.dispatchEvent(e);
          
        });
    });

}

function addErrors() {
    utils.waitForElement('#footer-email-send-form .response_error, #footer-email-send-form .value.errorclient, #footer-email-send-form .error').then(function () {
        //if(document.querySelector('.cv-footer-popup-email').value.length > 6 && document.querySelector('.cv-footer-popup-email').value.indexOf('@') != -1 && document.querySelector('.cv-footer-popup-email').value.indexOf('.') != -1){
        if (document.querySelector('.cv-popup-error')) {
          	console.log('Remove it');
            document.querySelector('.cv-popup-error').parentNode.removeChild(document.querySelector('.cv-popup-error'));
        }
        document.querySelector('.cv-footer-popup-email').insertAdjacentHTML('afterend', '<label class="cv-popup-error"></label>');
        if (document.querySelector('#footer-email-send-form .response_error') !== null) {
            document.querySelector('.cv-popup-error').innerHTML = document.querySelector('#footer-email-send-form .response_error').innerHTML;
        } else {
            document.querySelector('.cv-popup-error').innerHTML = document.querySelector('#footer-email-send-form .validation_error').innerHTML;
        }
        waitUntil(function () {
            return document.querySelector('#footer-email-send-form .response_error') == null && document.querySelector('#footer-email-send-form .validation_error') == null;
        }).then(function () {
            document.querySelector('.cv-footer-popup-email .cv-popup-error').parentNode.removeChild(document.querySelector('.cv-footer-popup-email .cv-popup-error'));

            addErrors();
        });
        //}else{
        //addErrors();
        //}
    });
}
