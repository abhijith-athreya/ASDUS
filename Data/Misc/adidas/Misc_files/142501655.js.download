//Disables all active pages, when view is switched
function disableAllPages(){
  var activePages = optimizely.get('state').getPageStates({
    "isActive": true
  });

  for (var index in activePages) {
    window['optimizely'] = window['optimizely'] || [];
    window.optimizely.push({
      type: 'page',
      pageName: activePages[index].apiName,
      isActive: false
    });
  }
  
  //Removes CSS which was added through Optimizely
  var removeOptiCss = document.querySelectorAll('style');
  for(var i=0; i < removeOptiCss.length; i++){
    if(removeOptiCss[i].attributes.length>0 && removeOptiCss[i].attributes[0].name.includes('data-optly-')){
      document.getElementsByTagName("head")[0].removeAttribute(removeOptiCss[i].attributes[0].name);
      removeOptiCss[i].parentNode.removeChild(removeOptiCss[i]);
    }
  }
};

//Check custom function and activate page is returned true;
function waitAndActivate(apiName, callback, interval){
  try{
    //Sets interval to 5 if none is set
    var checkInterval = interval || 5;
    //Checks if callback is set
    if (callback && typeof(callback) === "function") {
      setTimeout(function(){
        var resultCallback = callback();
        //Checks outcome of callback, else rechecks after increasing interval.
        if(resultCallback === true){
          activatePage(apiName);
        }else if(checkInterval < 500){
          checkInterval = checkInterval + 5;
          waitAndActivate(apiName, callback, checkInterval);
        }
      }, checkInterval);
    }
  }catch(error){
    console.log(error);
  }
}

//Activating experiments
function activatePage(apiName){
  window['optimizely'] = window['optimizely'] || [];
  window['optimizely'].push({
    type: "page",
    pageName: apiName,
    isActivate: true
  });
};

//Adding action listener on initial load
window["optimizely"] = window["optimizely"] || [];
window["optimizely"].push({
  type: "addListener",
  filter: {
    type: "lifecycle",
    name: "pageActivated"
  },

  // Add the pageActivated function as a handler.
  handler: pageActivated
});


function cvSetCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/; domain="+window.location.host.replace('www.', '.').replace('m.','.');
}

function cvGetCookie(cname) {
    var name = cname + "=";
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
    return "";
}

//if(document.cookie.indexOf("adidas_extra_opti_function") > -1){
window["dataLayerOptiTrigger"] = function(){
  try{
    var currentOptiCookieValue = cvGetCookie("cvOptiProfile");
    var currentProfile;

    if(currentOptiCookieValue !== ""){
      currentProfile = JSON.parse(currentOptiCookieValue);
    }else{
      currentProfile = {};
    }

    //------- START tracking for gender -------
    if(!optiGenderTracking && typeof(utag.data.filter_gender) !== "undefined"){
      var utagGender = utag.data.filter_gender;

      if(utagGender){
        if(currentProfile[utagGender]){
        currentProfile[utagGender]++;
        }else{
          currentProfile[utagGender] = 1;
        }
        
        if(utagGender === "MEN"){
          currentProfile["LVG"] = "M";
        }else if(utagGender === "WOMEN"){
          currentProfile["LVG"] = "W";
        }else if(utagGender === "KIDS"){
          currentProfile["LVG"] = "K";
        }

        cvSetCookie("cvOptiProfile", JSON.stringify(currentProfile), 30);
        optiGenderTracking = true;
      }
    }
    //------- END tracking for gender -------

    //------- START tracking categories of interest -------
    if(!optiCategoryTracking && typeof(utag.data.filter_sport) !== "undefined" || !optiCategoryTracking && typeof(utag.data.filter_brand) !== "undefined" && utag.data.filter_brand === "ORIGINALS"){
      var footballCategory = ["FOOTBALL"];
      var originalsCategory = ["LIFESTYLE", "ORIGINALS"];
      var runningCategory = ["RUNNING", "HARDLOPEN"];
      var trainingCategory = ["TRAINING"];

      var utagCategory = utag.data.filter_sport;
      var trackCategory = false;

      if(footballCategory.indexOf(utagCategory) > -1){
        trackCategory = "FOOTBALL";
      }else if(originalsCategory.indexOf(utagCategory) > -1 || originalsCategory.indexOf(utag.data.filter_brand) > -1){
        if(utag.data.filter_brand === "ORIGINALS"){
          trackCategory = "ORIGINALS";
        }
      }else if(runningCategory.indexOf(utagCategory) > -1){
        trackCategory = "RUNNING";
      }else if(trainingCategory.indexOf(utagCategory) > -1){
        trackCategory = "TRAINING";
      }

      if(trackCategory){
        if(currentProfile[trackCategory]){
        currentProfile[trackCategory]++;
        }else{
          currentProfile[trackCategory] = 1;
        }

        cvSetCookie("cvOptiProfile", JSON.stringify(currentProfile), 30);
        optiCategoryTracking = true;
      }
    }
    //------- END tracking categories of interest -------
  }catch(error){
    console.log('Global - Tracking issue');
  }
};
