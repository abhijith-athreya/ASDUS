// helper varants
var app = window.app;
var locale = app.constants.LOCALE;
var language = locale.split('_')[0]; // e.g. en
var country = locale.split('_')[1]; // e.g. GB
var dwBaseURL = window.location.protocol + '//' + window.location.hostname + '/on/demandware.store/Sites-adidas-' + country + '-Site/' + locale;

// check type of page based on URL
var pageType = (location.href === app.URLs.showCartURL)
  ? 'cart'
  : 'checkout';

// default checkoutObj
// has no default state per step
var checkoutSteps = [
  {
    name: 'Bag',
    link: app.URLs.showCartURL,
    state: null,
  },
  {
    name: 'Deliver',
    link: dwBaseURL + '/COShipping-Show',
    state: null,
  },
  {
    name: 'Payment',
    link: dwBaseURL + '/COSummary2-Start',
    state: null,
  },
  {
    name: 'Purchase',
    link: dwBaseURL + '/Summary-ShowConfirmation',
    state: null,
  },
];

// helper function to determine the current checkout step
// already passed checkout steps, and disabled checkout steps
// params: checkoutObj
// returns checkoutObj
function setCheckoutState(checkoutSteps) {
  var found = false;
  for (var i = 0; i < checkoutSteps.length; i++) {
    if (!~window.location.href.indexOf(checkoutSteps[i].link) && !found) {
      checkoutSteps[i].state = 'active';
    } else if (!!~window.location.href.indexOf(checkoutSteps[i].link)) {
      checkoutSteps[i].state = 'current';
      found = true;
    } else {
      checkoutSteps[i].state = 'disabled';
    }

    // update translations
    // if (pageType === 'checkout' && i > 0) {
    //   var checkoutStepName = document.querySelector('.co-checkoutprogressindicator .step-' + i).textContent;
    //   checkoutSteps[i].name = checkoutStepName.trim().replace(/[0-9\,\.]/, '');
    // }
  }

  return checkoutSteps;
}

// function that creates the new checkout progress bar
// params: checkoutObj
// returns: progressBar Node
function createNewProgressBar(checkoutSteps) {
  var progressList = document.createElement('ul');
        progressList.className = 'cv-progressbar';

  for (var i = 0; i < checkoutSteps.length; i++) {
    var progressListItem = document.createElement('li');
        progressListItem.className = 'cv-progress-item ' + checkoutSteps[i].state; 
  
    var listItemNumber = document.createElement('a');
        listItemNumber.className = 'cv-progress-item__stepnumber';
        listItemNumber.textContent = i + 1;
        listItemNumber.href = checkoutSteps[i].link;
    
    var listItemSeperator = document.createElement('span');
        listItemSeperator.className = 'cv-progress-item__separator';
        listItemSeperator.textContent = '.';
    
    var listItemStep = document.createElement('span');
        listItemStep.textContent = checkoutSteps[i].name;
        listItemStep.className = 'cv-progress-item__step';
    
    if (i === checkoutSteps.length - 1) {
      progressListItem.classList.add('final-step');
      var glyph = document.createElement('i');
      progressListItem.appendChild(glyph);
    } else {
      progressListItem.appendChild(listItemNumber);
      progressListItem.appendChild(listItemSeperator);
      progressListItem.appendChild(listItemStep);
    }
    progressList.appendChild(progressListItem);
  }

  return progressList;
}

var container = document.getElementById('container');
utils.waitForElement('#container').then(function(container) {
  checkoutSteps = setCheckoutState(checkoutSteps);
  if (!document.querySelector('.cv-progressbar')) {
    var progressBar = createNewProgressBar(checkoutSteps);
    container.insertBefore(progressBar, container.firstElementChild);
  }
});
}}, {"value": "<style>/* old progressbar */\n.co-checkoutprogressindicator {\n  display: none !important;\n}\n\n.cv-progressbar {\n  display: flex;\n  align-items: center;\n  margin: 15px 0;\n  font-weight: 700;\n  text-transform: uppercase;\n}\n\n.cv-progress-item {\n  margin-right: 40px;\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.cv-progress-item:first-of-type {\n  margin-left: 20px;\n}\n\n.cv-progress-item.final-step > i::after {\n  position: absolute;\n  top: 50%;\n  left: 0;\n  margin-top: -7px;\n  content: '\\e605'; /*tick icon*/\n  font-family: 'icomoonregular';\n  text-transform: none;\n  font-style: normal;\n  font-weight: 400;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  speak: none;\n  line-height: 1;\n  font-size: 1.4rem;\n}\n\n.cv-progress-item:not(:last-of-type)::after {\n  content: \"\";\n  display: block;\n  width: 20px;\n  height: 2px;\n  border-bottom: 1px solid #c8cbcc;\n  position: absolute;\n  transform: translate(40%, -50%);\n  left: 100%;\n  top: 45%;\n}\n\n.cv-progress-item.active::after, .cv-progress-item.current::after {\n  color: #000000;\n}\n\n.cv-progress-item__stepnumber {\n  display: block;\n  color: #000;\n  pointer-events: none;\n}\n\n.active .cv-progress-item__stepnumber  {\n\tpointer-events: all;\n}\n\n.cv-progress-item__separator {\n  display: none;\n  margin-right: 2px;\n}\n\n.cv-progress-item__step {\n  display: none;\n  text-align: center;\n}\n\n.cv-progress-item.current .cv-progress-item__stepnumber {\n  display: block;\n}\n\n.cv-progress-item.current .cv-progress-item__separator {\n  display: block;\n}\n\n.cv-progress-item.current .cv-progress-item__step {\n  display: block;\n}</style>", "selector": "head", "dependencies": [], "type": "append", "id": "FEF9C2AA-3254-4872-8258-49B0B7E15FBA"}]}], "name": "V1: New checkout progress bar"}], "audienceIds": ["or", "3423170166", "3385911413"], "changes": null, "id": "10560620028", "integrationSettings": null}], "id": "