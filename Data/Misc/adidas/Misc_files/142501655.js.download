function add_banner(prod_id){
	// Get data from product
  //var img_627 = document.querySelector('img.performance-item').cloneNode(1);
  //var img_627 = document.querySelectorAll('div[class*="thumbnails_container"] div')[1].childNodes[0].childNodes[0].cloneNode(1);
  var img_627 = document.querySelectorAll('div[class*="thumbnails_container"] div')[1].childNodes[0].cloneNode(1);
  var title_627 = document.querySelector('[data-auto-id="product-title"]').cloneNode(1);
  var size_627 = document.querySelector('[data-auto-id="size-selector"]').cloneNode(1);
  var bag_627 = document.querySelector('[data-auto-id="add-to-bag"]').cloneNode(1);
  
  if(document.querySelectorAll('.price-big').length > 0) {
   		  var price_627 = document.querySelector('.price-big').cloneNode(1);
  } else if (document.querySelectorAll('.price-sale-big').length > 0) {
   		  var price_627 = document.querySelector('.price-sale-big').cloneNode(1);
  }
  
  if(document.querySelectorAll('[data-auto-id="product-rating-review-count"]').length > 0) {
  	  var reviews_627 = document.querySelector('[data-auto-id="product-rating-review-count"]').cloneNode(1);
  		var stars_627 = document.querySelector('span[class*="glass_star_rating"]').cloneNode(1);
  		var rating_627 = document.querySelector('[class^="product_reviews"]').cloneNode(1);
  } else {
   		var reviews_627 = document.createElement('div');
  		var stars_627 = document.createElement('div');
  		var rating_627 = document.createElement('div');
  }

  // Give classes
  img_627.className += ' img_627';
  title_627.className += ' title_627';
  price_627.className += ' price_627';
  rating_627.className += ' rating_627';
  size_627.className += ' size_627';
  bag_627.className += ' bag_627';
  stars_627.className += ' stars_627';
  reviews_627.className += ' reviews_627';

  // Change element tag
  if(title_627.innerText.length > 35) {
  	  var titleTemp = document.createElement('h6');
    	titleTemp.innerHTML = title_627.innerHTML;
  		title_627 = titleTemp;
  } else {
     	var titleTemp = document.createElement('h4');
  		titleTemp.innerHTML = title_627.innerHTML;
  		title_627 = titleTemp;
  }

  // Create elements for sticky
  var div_img_627 = document.createElement('div');
  var npr_627 = document.createElement('div');
  var div_name_627 = document.createElement('div');
  var div_pricereview_627 = document.createElement('div');
  var div_price_627 = document.createElement('div');
  var div_review_627 = document.createElement('div');
  var div_stars_627 = document.createElement('div');
  var div_size_627 = document.createElement('div');
  var div_bag_627 = document.createElement('div');
  var left_627 = document.createElement('div');
  var right_627 = document.createElement('div');

  // Give elements classes
  div_img_627.className += 'div_img_627';
  npr_627.className += 'npr_627';
  div_name_627.className += 'div_name_627';
  div_pricereview_627.className += 'div_pricereview_627';
  div_price_627.className += 'div_price_627';
  div_review_627.className += 'div_review_627';
  div_stars_627.className += 'div_stars_627';
  div_size_627.className += 'div_size_627';
  div_bag_627.className += 'div_bag_627';
  left_627.className += 'left_627';
  right_627.className += 'right_627';

  // Create sticky and element containers
  var sticky = document.createElement('div');
  sticky.className += 'sticky_627';
  sticky.id = prod_id;
  
  sticky.appendChild(left_627);
  sticky.appendChild(right_627);

  left_627.appendChild(div_img_627);
  left_627.appendChild(npr_627);

  npr_627.appendChild(div_name_627);
  npr_627.appendChild(div_pricereview_627);

  div_pricereview_627.appendChild(div_price_627);
  div_pricereview_627.appendChild(div_stars_627);
  div_pricereview_627.appendChild(div_review_627);

  // Add data to the element containers
  div_img_627.appendChild(img_627);
  div_name_627.appendChild(title_627);
  div_price_627.appendChild(price_627);
  div_review_627.appendChild(reviews_627);
  div_stars_627.appendChild(stars_627);

  // Place sticky on the website
  document.querySelector('#app > div > div div[data-auto-id="pdp"]').appendChild(sticky);
  
  // Placement on scroll 
  var form = document.querySelector("form[class*='add_to_bag']");
  var order_information = form.parentNode;
  var right = document.querySelector('.right_627');
  var infor = document.querySelector('div[class*="order_information_"]');
  var formparent = form.parentNode;
  var offset = formparent.offsetTop + formparent.offsetHeight;
  var sizesheight;
  
  var placed = false;
  
  document.addEventListener("scroll", function() {
    	if(window.pageYOffset > offset && placed === false) {  
        	// show sticky bar. 
          right.appendChild(form);
        	
        	if(document.querySelectorAll('.sticky_627').length > 0) {
        		document.querySelector('.sticky_627').classList.add('sticky_show');
          }
        
        	var options = document.querySelector('.row.no-gutters div[class*="dropdown"] div[class*="wrapper"] > div[class*="options"]');
          options.style.width = '314px';
        	var sizesheight = options.offsetHeight;
      		options.style.top = sizesheight * -1 - 32; 
        
        	placed = true;
        
      } else if (window.pageYOffset <= offset && placed === true) {
					infor.insertBefore(form, infor.lastChild);
        	placed = false;
        
        	if(document.querySelectorAll('.sticky_627').length > 0) {
          	document.querySelector('.sticky_627').classList.remove('sticky_show');
          }
        
        	var options = document.querySelector('.row.no-gutters div[class*="dropdown"] div[class*="wrapper"] > div[class*="options"]');
					options.style.top = '50px';
      }
    
    
    	if(document.querySelectorAll('.sticky_627').length > 0) {
        	// fix stars for fast scroll
          if(document.querySelectorAll('[data-auto-id="product-rating-review-count"]').length > 0 && 
             document.querySelectorAll('.stars_627 svg').length < 1) {

            var reviews_627 = document.querySelector('[data-auto-id="product-rating-review-count"]').cloneNode(1);
            var stars_627 = document.querySelector('span[class*="glass_star_rating"]').cloneNode(1);
            var rating_627 = document.querySelector('[class^="product_reviews"]').cloneNode(1);

            stars_627.classList.add('stars_627');
            reviews_627.classList.add('reviews_627');

            document.querySelector('.div_stars_627').innerHTML = '';
            document.querySelector('.div_review_627').innerHTML = '';

            document.querySelector('.div_stars_627').appendChild(stars_627);
            document.querySelector('.div_review_627').appendChild(reviews_627);
          }

          // fix image for fast scroll
          if(document.querySelector('.div_img_627 img').getAttribute('data-auto-id') == 'placeholder') {
            var img_627 = document.querySelectorAll('div[class*="thumbnails_container"] div')[1].childNodes[0].cloneNode(1);
            document.querySelector('.div_img_627').innerHTML = '';    
            document.querySelector('.div_img_627').appendChild(img_627);
          }
      }
    
  });
  
  
  // Add to Bag functionality
  utils.waitForElement('.right_627 div[class*="add_to_bag_container"]').then( function() {
    document.querySelector('.right_627 div[class*="add_to_bag_container"]').onclick = function(e) {
      
      	if(document.querySelectorAll('.right_627 div[data-auto-id="size-selector"] div[data-auto-id="label"]').length > 0) {
					
        	var selectorText = document.querySelector('.right_627 div[data-auto-id="size-selector"] div[data-auto-id="label"]').innerText;
					var sizeregex = RegExp("(SELECT SIZE|KIES MAAT|WÄHLE DEINE GRÖSSE AUS|ELIGE TU TALLA|SCEGLI LA MISURA|CHOISIR UNE TAILLE)");
          
          if(sizeregex.test(selectorText)) {
            	var options = document.querySelector('.row.no-gutters div[class*="dropdown"] div[class*="wrapper"] > div[class*="options"]');
              options.style.width = '314px';
              var sizesheight = options.offsetHeight;
              options.style.top = sizesheight * -1 - 32; 
							document.querySelector('.right_627 div[data-auto-id="size-selector"] div[data-auto-id="label"]').click();
          } else {
						 	window.scrollTo(0, 0);
        			// force click on top add
        			document.querySelector('.btn.btn-bag').click();
          }
				
        }
    };
	});
  
  // Utag clicks
  document.querySelector('div[class*="add_to_bag_container"]').onclick = function(e) {
    if(document.querySelector('.right_627').innerHTML.length > 0) {
      // utag
      utag.link({
        event: "element",
        event_category: "PDP:627a",
        event_name: "addbag:clicked"
      });
    }
	};
	
  // Utag + size selector position fix
  document.querySelector('div[data-auto-id="size-selector"]').onclick = function(e) {    
    var options = document.querySelector('.row.no-gutters div[class*="dropdown"] div[class*="wrapper"] > div[class*="options"]');

    if(document.querySelector('.right_627').innerHTML.length > 0) {
      options.style.width = '314px';
      var sizesheight = options.offsetHeight;
      options.style.top = sizesheight * -1 - 32; 
    } else {
      options.style.top = '50px';
    }
    
    if(document.querySelector('.right_627').innerHTML.length > 0) {
      // utag
      utag.link({
        event: "element",
        event_category: "PDP:627a",
        event_name: "ssize:clicked"
      });
    }
  };
  
  document.querySelector('.reviews_627 .hidden-s').onclick = function() {
		document.querySelector('div[data-auto-id="glass-ratings-stack"]').scrollIntoView();
	};
  
}

var this_prod_id=false;
document.querySelector('[data-auto-id="added-to-bag-modal"]').parentNode.classList.add("cvShown_ATP_627");

if(document.querySelector("meta[itemprop='sku']")){
  this_prod_id = document.querySelector("meta[itemprop='sku']").getAttribute("content");
}

var banner_prod_id = document.getElementsByClassName("sticky_627").id;
add_banner(this_prod_id);
