ight: 100%;\n  padding: 0;\n  margin: 0;\n  color: #fff;\n  text-decoration: none;\n  position: relative;\n}\n\n.cv-footer-popup-submit a span {\n  display: none;\n}\n\n.cv-footer-popup-submit a:after {\n  content: '\\e628';\n  display: block;\n  font-family: 'icomoonregular';\n  text-transform: none;\n  font-style: normal;\n  font-weight: 400;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  speak: none;\n  line-height: normal;\n  width: 100%;\n  height: 2rem;\n  font-size: 1.3rem;\n  text-align: center;\n  padding-top: 3px;\n  position: absolute;\n  left: 0;\n  top: 50%;\n  margin: -1rem 0 0;\n}\n\n.cv-footer-popup-submit .signupandsave_submit {\n  display: block!important;\n}\n\n.cv-signup-complete {\n  text-align: center;\n}\n\n.cv-overlay {\n  height: 24%;\n  min-height:115px;\n  width: 100%;\n  position: fixed;\n  z-index: 100001;\n  bottom: 0;\n  left: 0;\n  overflow-y: hidden;\n  -webkit-transition: 1s;\n  -o-transition: 1s;\n  transition: 1s;\n}\n\n.cv-popup-error {\n  margin-bottom: 0!important;\n  color: red!important;\n}\n\n.cv-signup-complete .cv-popup-close {\n  top: -8px;\n  right: 10px;\n  position: absolute;\n}\n\n.cv-footer-popup > label, .cv-footer-popup > .legal-copy-content{\n  display:none;\n}\n.cv-footer-popup > .cv-popup-error{\n  display:block!important; \n}</style>", "selector": "head", "dependencies": [], "type": "append", "id": "a92d66402892433590c8e1b14449094c"}]}], "name": "Mobile signup popup"}], "audienceIds": ["or", "3423170166", "3385911413"], "changes": null, "id": "10600024923", "integrationSettings": null}], "id": "10602974883", "weightDistributions": null, "name": "ATP-1207 - Sign-up popup on mobile (Rerun) - AB Test - adidas - US - M - PLP - by Global", "groupId": null, "commitId": "10655171667", "decisionMetadata": null, "policy": "single_experiment", "changes": [{"dependencies": [], "type": "custom_code", "id": "86eafa1114d4478792f119d0b167280b", "value": function($){if(typeof(utag)=="object")